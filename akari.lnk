OUTPUT_FORMAT("elf32-i386")
ENTRY(AkariPreboot)
phys = 0x100000;
SECTIONS
{
	.text phys : AT (phys)
	{
		___kstart = .;
		code = .;
		*(.multiboot)
		*(.text)
		. = ALIGN(4096);
	}

	.data : AT (phys + (data - code))
	{
		data = .;
		*(.data)
		. = ALIGN(4096);
	}

	.bss : AT(phys + (bss - code))
	{
		bss = .;
		*(.bss)
		*(COMMON)
		. = ALIGN(4096);
	}
	___kend = .;
}

